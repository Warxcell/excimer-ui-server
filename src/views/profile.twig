{% extends "index.twig" %}

{% block title %}
    {{ profile.name }} on {{ profile.createdAt.toLocaleString() }}
{% endblock %}

{% block content %}
    <iframe src="https://www.speedscope.app/#profileURL={{ getUrl(profile.id) }}&title={{ profile.name }}"
            title="{{ profile.name }}"
            height="100%" width="100%">
    </iframe>
{% endblock %}